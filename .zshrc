#==============================================================#
##              Sheldon                                       ##
#==============================================================#
eval "$(sheldon source)"

#==============================================================#
##              history                                       ##
#==============================================================#
HISTFILE=~/.zsh_history
HISTSIZE=10000
SAVEHIST=100000
setopt hist_ignore_all_dups
setopt hist_no_store

#==============================================================#
##              starship                                      ##
#==============================================================#
eval "$(starship init zsh)"

